{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": [
    "${mod_authors}"
  ],
  "contact": {
    "homepage": "${mod_github}",
    "sources": "${mod_github}",
    "issues": "${mod_github}/issues"
  },
  "license": "MIT",
  "icon": "assets/${mod_id}/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "dev.wuffs.bcc.fabric.BetterCompatabilityCheckerFabric"
    ]
  },
  "mixins": [
    {
      "config": "bcc.mixins.json"
    }
  ],
  "depends": {
    "fabricloader": "${fabric_loader_version_range}",
    "fabric-api": "${fabric_api_version_range}",
    "minecraft": "${minecraft_version_range_sem}"
  }
}
